{
  "expo": {
    "name": "Real-time quotes",
    "slug": "real-time-quotes",
    "privacy": "public",
    "description": "A simple app showing market quotes and their details",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fufylevdev.real-time-quotes",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.fufylevdev.real_time_quotes",
      "versionCode": 1
    }
  }
}
